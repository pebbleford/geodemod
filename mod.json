{
    "geode": "4.7.0",
    "gd": {
        "win": "2.2074"
    },
    "id": "liam.gd-mod-menu",
    "name": "GD Mod Menu",
    "version": "v1.0.0",
    "developer": "Liam",
    "description": "Full-featured mod menu for Geometry Dash. Press Tab to open.",
    "settings": {
        "menu-title": {
            "type": "title",
            "name": "Menu Settings"
        },
        "menu-keybind": {
            "type": "string",
            "name": "Menu Toggle Key",
            "description": "Key to open/close the mod menu",
            "default": "Tab"
        },
        "player-title": {
            "type": "title",
            "name": "Player Hacks"
        },
        "noclip": {
            "type": "bool",
            "name": "Noclip",
            "description": "Pass through obstacles without dying",
            "default": false
        },
        "no-death-effect": {
            "type": "bool",
            "name": "No Death Effect",
            "description": "Disable the death animation",
            "default": false
        },
        "auto-retry": {
            "type": "bool",
            "name": "Auto Retry",
            "description": "Automatically restart the level on death",
            "default": false
        },
        "level-title": {
            "type": "title",
            "name": "Level Hacks"
        },
        "instant-complete": {
            "type": "bool",
            "name": "Instant Complete",
            "description": "Instantly complete any level",
            "default": false
        },
        "practice-music": {
            "type": "bool",
            "name": "Practice Music Hack",
            "description": "Play normal music in practice mode",
            "default": false
        },
        "no-pause-unfocus": {
            "type": "bool",
            "name": "No Pause on Unfocus",
            "description": "Game doesn't pause when you alt-tab",
            "default": false
        },
        "auto-play": {
            "type": "bool",
            "name": "Auto Play",
            "description": "Automatically clicks for you to beat the level",
            "default": false
        },
        "display-title": {
            "type": "title",
            "name": "Display Hacks"
        },
        "speedhack": {
            "type": "float",
            "name": "Speed Hack",
            "description": "Game speed multiplier (1.0 = normal)",
            "default": 1.0,
            "min": 0.1,
            "max": 5.0,
            "controls": {
                "arrows": true,
                "arrow-step": 0.1,
                "slider": true,
                "slider-step": 0.05
            }
        },
        "speedhack-enabled": {
            "type": "bool",
            "name": "Enable Speed Hack",
            "description": "Toggle the speed hack on or off",
            "default": false
        },
        "show-percent": {
            "type": "bool",
            "name": "Show Accurate Percent",
            "description": "Show accurate level percentage",
            "default": false
        },
        "no-particles": {
            "type": "bool",
            "name": "No Particles",
            "description": "Disable particle effects for better performance",
            "default": false
        },
        "bypass-title": {
            "type": "title",
            "name": "Bypass Hacks"
        },
        "no-transition": {
            "type": "bool",
            "name": "No Transition",
            "description": "Skip scene transition animations",
            "default": false
        },
        "unlock-all-icons": {
            "type": "bool",
            "name": "Unlock All Icons",
            "description": "Unlock every icon in the game",
            "default": false
        },
        "unlock-all-colors": {
            "type": "bool",
            "name": "Unlock All Colors",
            "description": "Unlock every color in the game",
            "default": false
        },
        "safe-mode": {
            "type": "bool",
            "name": "Safe Mode",
            "description": "Prevent cheated scores from being submitted",
            "default": true
        }
    }
}
